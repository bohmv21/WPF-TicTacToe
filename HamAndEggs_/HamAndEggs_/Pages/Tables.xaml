            <Button x:Name="_12" Content="Table 12" Grid.Row="2" Grid.Column="1" Margin="20" Click="Default_Click"></Button>
            <Button x:Name="_13" Content="Table 13" Grid.Row="2" Grid.Column="2" Margin="20" Click="Default_Click"></Button>
