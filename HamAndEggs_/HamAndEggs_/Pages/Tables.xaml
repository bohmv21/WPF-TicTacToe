<Page x:Class="HamAndEggs_.Pages.Tables"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:HamAndEggs_.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="500" d:DesignWidth="500"
      Title="Tables">

    <Grid>
        <Frame Background="White"/>
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
                <ColumnDefinition></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
                <RowDefinition></RowDefinition>
            </Grid.RowDefinitions>


            <Button x:Name="_1" Content="Table 1" Grid.Row="0" Grid.Column="0" Margin="20" Click="Default_Click"></Button>
            <Button x:Name="_2" Content="Table 2" Grid.Row="0" Grid.Column="1" Margin="20" Click="Default_Click"></Button>
            <Button x:Name="_3" Content="Table 3" Grid.Row="0" Grid.Column="2" Margin="20" Click="Default_Click"></Button>
            <Button x:Name="_4" Content="Table 4" Grid.Row="0" Grid.Column="3" Margin="20" Click="Default_Click"></Button>
            <Button x:Name="_5" Content="Table 5" Grid.Row="0" Grid.Column="4" Margin="20" Click="Default_Click"></Button>

            <Button x:Name="_6" Content="Table 6" Grid.Row="1" Grid.Column="0" Margin="20" Click="Default_Click"></Button>
            <Button x:Name="_7" Content="Table 7" Grid.Row="1" Grid.Column="1" Margin="20" Click="Default_Click"></Button>
            <Button x:Name="_8" Content="Table 8" Grid.Row="1" Grid.Column="2" Margin="20" Click="Default_Click"></Button>
